{
    "name": "mousel68/oauth2-mousel-fitbit",
    "description": "Fitbit OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Chen",
            "email": "dan@djc.me",
            "homepage": "https://www.djc.me"
        },
        {
            "name": "Tim Mousel",
            "email": "mouseltim@gmail.com",
